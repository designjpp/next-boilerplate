{
  "name": "nextjs-boilerplate",
  "license": "MIT",
  "private": "false",
  "version": "0.3.0",
  "author": "soheil.movahhedan@gmail.com",
  "scripts": {
    "/////////////////// basic //////////////////////////////////////////": "",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "postinstall": "husky install",
    "postbuild": "next-sitemap",
    "/////////////////// test & format //////////////////////////////////": "",
    "test": "yarn jest",
    "lint": "next lint",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
    "/////////////////// tailwind ///////////////////////////////////////": "",
    "clone:tailwind": "./tailwind.generate.config.js && next lint --fix -d src/constants/css",
    "/////////////////// cypress ////////////////////////////////////////": "",
    "dev:cypress": "yarn dev & yarn cypress open",
    "build:cypress": "yarn build && (yarn start & yarn cypress run)",
    "clean:cypress": "kill -9 $(lsof -iTCP -sTCP:LISTEN | grep 3000 | awk '{print $2}');",
    "/////////////////// release ////////////////////////////////////////": "",
    "release:commit": "yarn type-check && yarn lint && yarn test",
    "release:build": "yarn release:commit && yarn build:cypress",
    "release:clean": "yarn clean:cypress"
  },
  "sideEffects": [
    "./public/*",
    "./src/pages/*"
  ],
  "dependencies": {
    "@emotion/css": "^11.5.0",
    "@emotion/react": "^11.5.0",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@sentry/nextjs": "^6.13.3",
    "axios": "^0.24.0",
    "next": "^12.0.2",
    "next-seo": "^4.28.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "swr": "^1.0.1",
    "ua-parser-js": "^1.0.2"
  },
  "devDependencies": {
    "@axe-core/react": "^4.2.2",
    "@babel/core": "^7.14.3",
    "@emotion/babel-plugin": "^11.3.0",
    "@next/eslint-plugin-next": "^12.0.2",
    "@testing-library/dom": "^8.2.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/react-hooks": "^7.0.1",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/jest-axe": "^3.5.2",
    "@types/node": "^16.11.6",
    "@types/react": "^17.0.5",
    "@types/react-test-renderer": "^17.0.1",
    "@types/tailwindcss": "^2.2.1",
    "@types/ua-parser-js": "^0.7.36",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "autoprefixer": "^10.3.3",
    "axios-mock-adapter": "^1.20.0",
    "babel-jest": "^27.1.0",
    "cypress": "^8.3.1",
    "eslint": "<8.0.0",
    "eslint-config-next": "^12.0.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-tailwind": "^0.2.1",
    "fetch": "^1.1.0",
    "husky": "^7.0.4",
    "jest": "^27.1.0",
    "jest-axe": "^5.0.1",
    "jest-cli": "^27.1.0",
    "lint-staged": "^11.0.0",
    "next-page-tester": "^0.30.0",
    "next-sitemap": "^1.6.179",
    "postcss": "^8.3.6",
    "prettier": "^2.3.0",
    "react-test-renderer": "^17.0.2",
    "sass": "^1.43.4",
    "tailwindcss": "^2.2.8",
    "ts-jest": "^27.0.5",
    "typescript": "~4.4.4"
  }
}
