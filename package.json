{
  "name": "nextjs-boilerplate",
  "license": "MIT",
  "private": "false",
  "version": "0.3.0",
  "author": "soheil.movahhedan@gmail.com",
  "scripts": {
    "/////////////////// basic //////////////////////////////////////////": "",
    "dev": "NODE_ENV=development next dev",
    "build": "NODE_ENV=production next build",
    "start": "NODE_ENV=production next start",
    "postinstall": "husky install",
    "postbuild": "next-sitemap",
    "/////////////////// test & format //////////////////////////////////": "",
    "test": "yarn jest",
    "lint": "next lint",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
    "/////////////////// tailwind ///////////////////////////////////////": "",
    "clone:tailwind": "./tailwind.generate.config.js && next lint --fix -d src/ui/utils",
    "/////////////////// cypress ////////////////////////////////////////": "",
    "dev:cypress": "yarn dev & yarn cypress open",
    "build:cypress": "yarn build && (yarn start & yarn cypress run)",
    "clean:cypress": "kill -9 $(lsof -iTCP -sTCP:LISTEN | grep 3000 | awk '{print $2}');",
    "/////////////////// storybook //////////////////////////////////////": "",
    "storybook": "start-storybook -s ./public -p 6006",
    "build:storybook": "build-storybook -s public",
    "/////////////////// release ////////////////////////////////////////": "",
    "release:commit": "yarn type-check && yarn lint && yarn test",
    "release:build": "yarn release:commit && yarn build:cypress",
    "release:clean": "yarn clean:cypress"
  },
  "sideEffects": [
    "./public/*",
    "./src/pages/*"
  ],
  "dependencies": {
    "@sentry/nextjs": "^6.12.0",
    "axios": "^0.21.4",
    "next": "^11.1.0",
    "next-seo": "^4.28.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "swr": "^1.0.0",
    "ua-parser-js": "^0.7.28"
  },
  "devDependencies": {
    "@axe-core/react": "^4.2.2",
    "@babel/core": "^7.14.3",
    "@next/eslint-plugin-next": "^11.1.0",
    "@storybook/addon-a11y": "^6.3.8",
    "@storybook/addon-actions": "^6.3.8",
    "@storybook/addon-docs": "^6.3.8",
    "@storybook/addon-essentials": "^6.3.8",
    "@storybook/addon-links": "^6.3.8",
    "@storybook/react": "^6.3.8",
    "@storybook/theming": "^6.3.8",
    "@styled-jsx/plugin-sass": "^4.0.1",
    "@testing-library/dom": "^8.2.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/react-hooks": "^7.0.1",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/jest-axe": "^3.5.2",
    "@types/node": "^15.3.0",
    "@types/react": "^17.0.5",
    "@types/react-test-renderer": "^17.0.1",
    "@types/tailwindcss": "^2.2.1",
    "@types/ua-parser-js": "^0.7.36",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.0.0",
    "autoprefixer": "^10.3.3",
    "axios-mock-adapter": "^1.20.0",
    "babel-jest": "^27.1.0",
    "cypress": "^8.3.1",
    "eslint": "^7.26.0",
    "eslint-config-next": "11.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-tailwind": "^0.2.1",
    "fetch": "^1.1.0",
    "husky": "^6.0.0",
    "jest": "^27.1.0",
    "jest-axe": "^5.0.1",
    "jest-cli": "^27.1.0",
    "lint-staged": "^11.0.0",
    "lost": "^8.3.0",
    "next-page-tester": "^0.28.0",
    "next-sitemap": "^1.6.179",
    "postcss": "^8.3.6",
    "postcss-nested": "^5.0.6",
    "prettier": "^2.3.0",
    "react-test-renderer": "^17.0.2",
    "sass": "^1.38.1",
    "styled-jsx-plugin-postcss": "^4.0.1",
    "tailwindcss": "^2.2.8",
    "ts-jest": "^27.0.5",
    "typescript": "~4.3"
  }
}
