{
  "name": "next-start",
  "license": "MIT",
  "private": "false",
  "version": "0.3.0",
  "author": "soheil.movahhedan@gmail.com",
  "scripts": {
    "dev": "NODE_ENV=development next dev",
    "dev:cypress": "yarn dev & yarn cypress open",
    "build": "NODE_ENV=production next build",
    "start": "NODE_ENV=production next start",
    "postinstall": "husky install",
    "lint": "next lint",
    "test": "yarn type-check && yarn jest",
    "cypress-check": "yarn build && (yarn start & yarn cypress run)",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
    "release-check": "yarn test && yarn cypress-check",
    "after-check": "kill -9 $(lsof -iTCP -sTCP:LISTEN | grep 3000 | awk '{print $2}');"
  },
  "sideEffects": [
    "./public/*",
    "./src/pages/*"
  ],
  "dependencies": {
    "@headlessui/react": "^1.4.1",
    "@sentry/nextjs": "^6.12.0",
    "mongoose": "^6.0.5",
    "next": "^11.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-query": "^3.21.0",
    "swr": "^1.0.0",
    "ua-parser-js": "^0.7.28"
  },
  "devDependencies": {
    "@axe-core/react": "^4.2.2",
    "@babel/core": "^7.14.3",
    "@next/eslint-plugin-next": "^11.1.0",
    "@styled-jsx/plugin-sass": "^4.0.1",
    "@testing-library/dom": "^8.2.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/react-hooks": "^7.0.1",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/jest-axe": "^3.5.2",
    "@types/node": "^15.3.0",
    "@types/react": "^17.0.5",
    "@types/react-test-renderer": "^17.0.1",
    "@types/tailwindcss": "^2.2.1",
    "@types/ua-parser-js": "^0.7.36",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "autoprefixer": "^10.3.3",
    "babel-jest": "^27.1.0",
    "cypress": "^8.3.1",
    "eslint": "^7.26.0",
    "eslint-config-next": "11.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-tailwind": "^0.2.1",
    "fetch": "^1.1.0",
