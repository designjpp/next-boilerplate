    "jest-fetch-mock": "^3.0.3",
    "lint-staged": "^11.0.0",
    "lost": "^8.3.0",
    "msw": "^0.35.0",
    "next-page-tester": "^0.28.0",
    "postcss": "^8.3.6",
    "postcss-nested": "^5.0.6",
    "prettier": "^2.3.0",
    "react-test-renderer": "^17.0.2",
    "sass": "^1.38.1",
    "styled-jsx-plugin-postcss": "^4.0.1",
